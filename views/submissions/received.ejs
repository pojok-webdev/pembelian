<script>
  $('#btnAddVendorComparation').on('click',function(){
    $('#add-vendor-comparation').modal()
  })
  $('#vendor').select2({
      ajax:{
        url:'/select2datafactory3',
        type:'post',
        data:function(params){
          var query = {
            search:params.hasOwnProperty('term')?params.term:'',type:'public',
            model:'proposedvendor'
          }
          console.log('proposedvendor',query)
          return query
        },
        dataType:'json',
        processResults:function(data){
          console.log('proposedvendor',data)
          return data
        }
      }
    })
  saveVendorComparation = _=>{
    $.ajax({
      url:'/crud',
      type:'post',
      data:{
        tableName:'fakesubmissiondetails_vendors',
        crudtype:'create',
        cols:[
          {key:'submission_detail_id',val:submission_detail_id},
          {key:'vendor_id',val:$('#vendor').val()},
          {key:'price',val:$('#dpp').val()},
          {key:'ppn',val:$('#ppn').val()},
          {key:'ongkir',val:$('#ongkir').val()},
          {key:'createuser',val:username}
        ]
      }
    })
    .done(res=>{
      console.log('Res',res)
    })
    .fail(err=>{
      console.log('Error save vendor comparation',err)
    })
  }
  $('#btnSaveVendorComparation').on('click',function(){
    console.log("vendor",$('#vendor').val())
    if(!$('#vendor').val()){
      alert("Vendor harus diisi")
    }else{
      saveVendorComparation()
    }
  })
</script>
<script>
  $('.changetotal').on('keyup',function(){
    console.log('Amount',$('#amount').val())
    $('#ppn').val(0.11*$('#dpp').val())
    ppn = $('#ppn').val()
    total = (1*$('#dpp').val()+1*ppn+1*$('#ongkir').val())*$('#amount').val()
    $('#total').val(total)
  })
  $('.changetotal').on('change',function(){
    console.log('Amount',$('#amount').val())
    $('#ppn').val(0.11*$('#dpp').val())
    ppn = $('#ppn').val()
    total = (1*$('#dpp').val()+1*ppn+1*$('#ongkir').val())*$('#amount').val()
    $('#total').html(total)
  })
</script>
